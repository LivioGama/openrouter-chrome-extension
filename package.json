{
  "name": "openrouter-chrome-extension",
  "version": "1.0.0",
  "description": "OpenRouter CSV Cost Analyzer Chrome Extension",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "package": "bun run build && zip -r extension.zip dist/* && mv extension.zip ~/Desktop",
    "wss": "bunx wss-ext-reloader --port 3001",
    "dev:full": "concurrently --kill-others-on-fail \"bun run wss\" \"bun run dev\""
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "chokidar": "^4.0.3",
    "chokidar-cli": "^3.0.0",
    "chrome-cli": "^1.1.1",
    "concurrently": "^9.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "keywords": [
    "chrome-extension",
    "openrouter",
    "csv-analyzer"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "vite-plugin-extension-reloader": "^1.1.6"
  }
}
